// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "POSTGRES://admin:admin@localhost:5432/database"
}

model All {
  id     Int     @id @default(autoincrement())
  status Boolean
  name   String
}
